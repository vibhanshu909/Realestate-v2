		"postbuild": "cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/;cp prisma/schema.prisma .vercel_build_output/functions/node/render/;ls .vercel_build_output/functions/node/render/"
	},
	"devDependencies": {
		"@playwright/test": "1.21.1",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-vercel": "^1.0.0-next.49",
		"@sveltejs/kit": "1.0.0-next.320",
		"@types/bcryptjs": "^2.4.2",
		"@types/cookie": "^0.5.1",
		"@types/jsonwebtoken": "^8.5.8",
		"@typescript-eslint/eslint-plugin": "^5.20.0",
		"@typescript-eslint/parser": "^5.20.0",
		"autoprefixer": "^10.4.4",
		"daisyui": "^2.14.1",
		"eslint": "^8.14.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^3.4.1",
		"mongodb": "^4.5.0",
		"postcss": "^8.4.12",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"prettier-plugin-tailwindcss": "^0.1.10",
		"prisma": "^3.12.0",
		"svelte": "^3.47.0",
		"svelte-check": "^2.7.0",
		"svelte-preprocess": "^4.10.6",
		"tailwindcss": "^3.0.24",
		"tslib": "^2.4.0",
		"typescript": "~4.6.3"
	},
	"type": "module",
	"dependencies": {
		"@prisma/client": "^3.12.0",
		"bcryptjs": "^2.4.3",
		"cookie": "^0.5.0",
		"date-fns": "^2.28.0",
		"jsonwebtoken": "^8.5.1"
	}
}
